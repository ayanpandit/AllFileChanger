{
  "name": "allfilechanger-image-converter-api",
  "version": "2.0.0",
  "description": "High-performance production image converter API for AllFileChanger - supports JPG, PNG, WebP, AVIF, HEIF, HEIC, GIF, ICO, SVG formats with enterprise-grade optimizations",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "node -e \"console.log('Server test passed')\"",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "image",
    "converter",
    "sharp",
    "api",
    "jpg",
    "png", 
    "webp",
    "avif",
    "heif",
    "heic", 
    "gif",
    "ico",
    "svg",
    "compression",
    "optimization",
    "render",
    "production",
    "allfilechanger"
  ],
  "author": "AllFileChanger Team",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "heic-convert": "^2.1.0",
    "multer": "^2.0.2", 
    "sharp": "^0.33.5",
    "svg2img": "^1.0.0-beta.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AllFileChanger/image-converter-api"
  },
  "bugs": {
    "url": "https://github.com/AllFileChanger/image-converter-api/issues"
  },
  "homepage": "https://allfilechanger.netlify.app/",
  "deploy": {
    "platform": "render",
    "buildCommand": "npm install --production",
    "startCommand": "npm run prod",
    "healthCheck": "/health",
    "environment": "production"
  }
}
